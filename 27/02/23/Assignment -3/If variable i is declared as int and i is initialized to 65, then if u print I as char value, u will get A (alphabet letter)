#include <stdio.h>
#include <stdlib.h>

int main()
{
    int i,j,rows,k;
    printf("Enter the number of rows: ");
    scanf("%d",&rows);
    i=1;
    while(i<=rows){
            j=1;
        while(j<=i){
        printf("%c",'A'-1+j);
        j++;
    }
    i++;
    printf("\n");
    }


    for (k= 65; k <= 90; k++) {
    printf("%c ", k);

    }
}
